generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id @db.VarChar(255)
  login     String  @unique @db.VarChar(255)
  password  String  @db.VarChar(255)
  age       Int
  isDeleted Boolean @default(false)

  @@map("Users")
}
